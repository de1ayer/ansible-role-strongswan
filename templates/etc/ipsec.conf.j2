# << Ansible managed file >>
# {{strongswan_config_file}} - strongSwan IPsec configuration file

config setup
{% for k,v in strongswan_config_setup|dict2items %}
	{{k}}={{v}}
{% endfor %}

conn %default
{% for k,v in strongswan_conn_default|dict2items %}
	{{k}}={{v}}
{% endfor %}

{% for conn,conn_settings in strongswan_conns|dict2items %}
conn {{conn}}
{%   for k,v in conn_settings|dict2items %}
	{{k}}={{v}}
{%   endfor %}

{% endfor %}
